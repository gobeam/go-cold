    "clsx": "^1.1.1",
    "connected-react-router": "^6.5.2",
    "fontfaceobserver": "^2.1.0",
    "history": "^4.9.0",
    "hoist-non-react-statics": "^3.3.0",
    "immer": "^8.0.1",
    "intl": "^1.2.5",
    "invariant": "^2.2.4",
    "lodash": "^4.17.15",
    "offline-plugin": "^5.0.7",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-app-polyfill": "^1.0.2",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-intl": "2.8.0",
    "react-redux": "^7.1.1",
